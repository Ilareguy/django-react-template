{
  "name": "template-project",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "build:dev": "webpack --mode development ./src/index.js -o ./static/ && yarn run css:dev",
    "build:prod": "webpack --mode production ./src/index.js -o ./static/ && yarn run css:prod",
    "css:dev": "cross-env CSS_PURGE=false tailwindcss build src/css/project.css -o ./static/project.css",
    "css:prod": "cross-env CSS_PURGE=true tailwindcss build src/css/project.css -o ./static/project.css",
    "storybook": "yarn run css:dev && start-storybook -s ./"
  },
  "keywords": [],
